{
  "name": "nx-giant",
  "version": "0.0.0",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && nx run-many --target=lint --all --parallel=4"
    }
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "dev:backend": "nx serve backend",
    "build": "nx build",
    "test": "nx test",
    "prepare": "husky install",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.5",
    "@angular/cdk": "16.0.4",
    "@angular/common": "16.0.5",
    "@angular/compiler": "16.0.5",
    "@angular/core": "16.0.5",
    "@angular/forms": "16.0.5",
    "@angular/material": "16.0.4",
    "@angular/platform-browser": "16.0.5",
    "@angular/platform-browser-dynamic": "16.0.5",
    "@angular/router": "16.0.5",
    "@nestjs/common": "9.1.1",
    "@nestjs/core": "9.1.1",
    "@nestjs/platform-express": "9.1.1",
    "@ngrx/component-store": "15.3.0",
    "@nrwl/angular": "16.3.2",
    "@types/uuid": "^9.0.0",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.5",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "16.0.5",
    "@angular/language-service": "16.0.5",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.1.1",
    "@nrwl/cypress": "16.3.2",
    "@nrwl/eslint-plugin-nx": "16.3.2",
    "@nrwl/jest": "16.3.2",
    "@nrwl/js": "16.3.2",
    "@nrwl/linter": "16.3.2",
    "@nrwl/nest": "16.3.2",
    "@nrwl/node": "16.3.2",
    "@nrwl/webpack": "16.3.2",
    "@nrwl/workspace": "16.3.2",
    "@types/jest": "29.4.4",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "autoprefixer": "^10.4.0",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.2",
    "jest": "29.4.3",
